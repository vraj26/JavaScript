//declaring variable to serve as numbers
let num1 = 8;
let num2 = 8;

//if num1 is greater than num2, print num1 is greater than num2
if(num1 > num2){
  console.log(num1 + ' is greater than ' + num2);
} 
//if num2 is greater than num1, print num2 is greater than num1
else if(num2 > num1) {
  console.log(num2 + ' is greater than ' + num1);
} 
//else statement, meaning there is only 1 more possibility, num1 is equal to num 2
else {
  console.log(num1 + ' is equal to ' + num2)
}
